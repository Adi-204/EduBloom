<%- include('partials/header.ejs') %>
<img style="z-index: -1;"
src="https://images.unsplash.com/photo-1517433456452-f9633a875f6f?auto=format&fit=crop&q=80&w=1632&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" alt="">
<div class="signcontainer">
<h2 class="sign">Sign Up</h2>
    <form class="sign-form" action="/signup" method="post">
        <label for="username">Username:</label>
        <input type="text" id="username" name="username" required><br><br>

        <label for="email">Email:</label>
        <input type="email" id="email" name="email" required><br><br>

        <label for="password">Password:</label>
        <input type="password" id="password" name="password" required><br><br>
        <div class="form-container"><button type="submit">Submit</button></div>
    </form>
</div>
<%- include('partials/footer.ejs') %>

